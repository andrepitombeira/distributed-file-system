import br.sd.servicos.andre.ServicoDiretorio;
import java.rmi.Naming;

/**
 *
 * @author andre
 */
public class Diretorio {

    public Diretorio() {

        try {
            ServicoDiretorio sd = (ServicoDiretorio) new DiretorioImpl();
            Naming.bind("rmi://localhost:1098/ServidorDiretorio", sd);
        } catch (Exception e) {
        }

 }
    public static void main(String[] args){
        new Diretorio();
    }

}



