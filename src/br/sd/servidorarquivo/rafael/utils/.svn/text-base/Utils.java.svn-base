package br.sd.servidorarquivo.rafael.utils;

/**
 *
 * @author iaufc37.lima
 */
public class Utils {

    public static String formataData(java.util.Calendar data) {
        java.text.SimpleDateFormat format = new java.text.SimpleDateFormat("dd-MM-yyyy HH:mm:ss");
        return format.format(data.getTime());
    }
}
