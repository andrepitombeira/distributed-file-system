package br.sd.ligador.andre;

import java.rmi.Naming;

/**
 *
 * @author andre
 */
public class ServidorServico {

    public ServidorServico(){

        try {
            Servico s = (Servico) new ServidorServicoImpl();
            Naming.bind("rmi://localhost:1099/ServidorServico", s);
        } catch (Exception e) {
        }

    }

    public static void main(String[] args){
        new ServidorServico();
    }

}
